
<div class="container-fluid" ng-cloak>

    <header class="row site-header">
        <nav class="col-md-10 col-md-offset-1">
            <div class="row">
                <h1>AngularPlayGround Template</h1>
            </div>
            <div class="row">
                <ul class="list-inline">
                    <li>
                        <a href="#/home">Home</a>
                    </li>
                    <li>
                        <a href="#/todomanager">Todo Manager</a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    
    <div class="row site-body" ng-controller="mainBodyCtrl">
        <div class="col-md-10 col-md-offset-1" ng-view>
            <table>
                <tr><th>row number</th></tr>
                <tr ng-repeat="i in [0, 1, 2, 3, 4, 5, 6, 7]">
                    <td ng-repeat="j in [0,1,2,3,4,5,6,7]">{{(i+1)*j}}</td>
                </tr>
            </table>
        </div>
    </div>

    <div class="row site-footer">
        <div class="col-md-10 col-md-offset-1">
            <footer>
                <div class="text-right">
                    Your Footer Here
                </div>
            </footer>
        </div>
    </div>
</div>
